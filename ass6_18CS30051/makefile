asmGenerate: comp
	./comp 1 > ass6_18CS30051_quads1.out
	./comp 2 > ass6_18CS30051_quads2.out
	./comp 3 > ass6_18CS30051_quads3.out
	./comp 4 > ass6_18CS30051_quads4.out
	./comp 5 > ass6_18CS30051_quads5.out

comp: lex.yy.o y.tab.o ass6_18CS30051_translator.o ass6_18CS30051_target_translator.o
	g++ lex.yy.o y.tab.o ass6_18CS30051_translator.o \
	ass6_18CS30051_target_translator.o -lfl -o comp

ass6_18CS30051_target_translator.o: ass6_18CS30051_target_translator.cxx
	g++ -c ass6_18CS30051_target_translator.cxx

ass6_18CS30051_translator.o: ass6_18CS30051_translator.cxx ass6_18CS30051_translator.h
	g++ -c ass6_18CS30051_translator.h
	g++ -c ass6_18CS30051_translator.cxx

lex.yy.o: lex.yy.c
	g++ -c lex.yy.c

y.tab.o: y.tab.c
	g++ -c y.tab.c

lex.yy.c: ass6_18CS30051.l y.tab.h ass6_18CS30051_translator.h
	flex ass6_18CS30051.l

y.tab.c: ass6_18CS30051.y
	yacc -dtv ass6_18CS30051.y -W

y.tab.h: ass6_18CS30051.y
	yacc -dtv ass6_18CS30051.y -W
	
clean:
	rm lex.yy.c\
	   y.tab.c\
	   y.tab.h\
	   y.output\
	   lex.yy.o\
	   y.tab.o\
	   ass6_18CS30051_translator.o\
	   test1\
	   test2\
	   test3\
	   test4\
	   test5\
	   y.output\
	   comp\
	   ass6_18CS30051_target_translator.o\
	   ass6_18CS30051_translator.h.gch\
	   ass6_18CS30051_quads1.out\
	   ass6_18CS30051_quads2.out\
	   ass6_18CS30051_quads3.out\
	   ass6_18CS30051_quads4.out\
	   ass6_18CS30051_quads5.out\
	   libass2_18CS30051.a\
	   ass6_18CS30051_1.o\
	   ass2_18CS30051.o\
	   ass6_18CS30051_2.o\
	   ass6_18CS30051_3.o\
	   ass6_18CS30051_4.o\
	   ass6_18CS30051_5.o

#Test files
test1: ass6_18CS30051_1.o libass2_18CS30051.a
	gcc ass6_18CS30051_1.o -o test1 -L. -lass2_18CS30051 -no-pie
ass6_18CS30051_1.o: myl.h
	gcc -c ass6_18CS30051_1.s

test2: ass6_18CS30051_2.o libass2_18CS30051.a
	gcc ass6_18CS30051_2.o -o test2 -L. -lass2_18CS30051 -no-pie
ass6_18CS30051_2.o: myl.h
	gcc -c ass6_18CS30051_2.s

test3: ass6_18CS30051_3.o libass2_18CS30051.a
	gcc ass6_18CS30051_3.o -o test3 -L. -lass2_18CS30051 -no-pie
ass6_18CS30051_3.o: myl.h
	gcc -c ass6_18CS30051_3.s

test4: ass6_18CS30051_4.o libass2_18CS30051.a
	gcc ass6_18CS30051_4.o -o test4 -L. -lass2_18CS30051 -no-pie
ass6_18CS30051_4.o: myl.h
	gcc -c ass6_18CS30051_4.s

test5: ass6_18CS30051_5.o libass2_18CS30051.a
	gcc ass6_18CS30051_5.o -o test5 -L. -lass2_18CS30051 -no-pie
ass6_18CS30051_5.o: myl.h
	gcc -c ass6_18CS30051_5.s


libass2_18CS30051.a: ass2_18CS30051.o
	ar -rcs libass2_18CS30051.a ass2_18CS30051.o

ass2_18CS30051.o: ass2_18CS30051.c myl.h
	gcc -c ass2_18CS30051.c